<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="Jornadas">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Jornada" type="tJornada" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="tJornada">
        <xs:sequence>
            <xs:element name="Enfrentamiento" type="tEnfrentamiento" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="Numero_Jornada" type="xs:int"/>
        <xs:attribute name="Codigo_Competencion" type="xs:int"/>
        <xs:attribute name="Codigo_Jornada" type="xs:int"/>
    </xs:complexType>

    <xs:complexType name="tEnfrentamiento">
        <xs:sequence>
            <xs:element name="Codigo_Enfrentamiento" type="xs:int"/>
            <xs:element name="Equipo_Local" type="xs:string"/>
            <xs:element name="Equipo_Visitante" type="xs:string"/>
            <xs:element name="Ganador" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
